<h1>Home#index</h1>
<div id="map-canvas" style="width: 400px; height: 400px">
	<div id= "map">

	</div>
</div>

<form id="calculate-route" name="calculate-route" action="#" method="get" >
  <input type="hidden" id="from" name="from" required="required" placeholder="An address" size="30" />
  <a class = "btn btn-primary" id="from-link" href="#">Start Riding</a>
  <br />

  <input type="hidden" id="to" name="to" required="required" placeholder="Another address" size="30" />
  <a class = "btn btn-secondary" id="to-link" href="#">Finish Riding</a>
  <br />

  <input type="submit" value="See My AirCredit">
</form>
      <br>
